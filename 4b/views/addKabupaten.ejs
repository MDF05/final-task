<%- contentFor("css")  %>

<%- contentFor("body")  %>
<div class="container-fluid d-flex justify-content-center mb-5">
    <div class="col-lg-6 col-md-9 col-11 mt-5 bg-glasses rounded-3">
        <div>
            <h1 class="d-flex justify-content-center color-skyblue">Kabupaten</h1>
        </div>
        <form action="/register" method="post" enctype="multipart/form-data"
            class="form-control form bg-transparent border-0">
            <div class="d-flex flex-column color-skyblue gap-1 mb-4">
                <label for="name" id="label-name">name</label>
                <input type="text" name="name" id="name" class="" required>
            </div>
            <div class="d-flex flex-column color-skyblue gap-1 mb-4">
                <label for="provinsi" id="provinsi_id">asal provinsi</label>
                <select name="provinsi_id" id="">
                    <option></option>
                    <option value="aceh">aceh</option>
                    <option value="meda">medan</option>
                </select>

            </div>
            <div class="d-flex flex-column color-skyblue gap-1 mb-4">
                <label for="diresmikan" id="label-diresmikan">diresmikan</label>
                <input type="text" name="diresmikan" id="diresmikan" class="" required>
            </div>
            <div class="d-flex flex-column color-skyblue gap-1 mb-4">
                <label for="photo" id="label-photo">photo</label>
                <input type="file" name="photo" id="photo" class="bg-white" required>
            </div>
            <div>
            </div>
            <div class="d-flex flex-column align-items-end gap-1 mb-4">
                <button type="submit" class="btn btn-outline-primary w-25">submit</button>
            </div>
        </form>
    </div>
</div>

<%- contentFor("js")  %>