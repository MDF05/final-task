<%- contentFor("css")  %>

<%- contentFor("body")  %>
<div class="container-fluid d-flex justify-content-center mb-5">
    <div class="col-lg-6 col-md-9 col-11 mt-5 bg-glasses rounded-3">
        <div>
            <h1 class="d-flex justify-content-center color-skyblue">Add Provinsi</h1>
        </div>
        <form action="/provinsi" method="post" enctype="multipart/form-data"
            class="form-control form bg-transparent border-0">
            <div class="d-flex flex-column color-skyblue gap-1 mb-4">
                <label for="nama" id="label-nama">nama</label>
                <input type="text" name="nama" id="nama" class="" required>
            </div>
            <div class="d-flex flex-column color-skyblue gap-1 mb-4">
                <label for="diresmikan" id="label-diresmikan">diresmikan</label>
                <input type="date" name="diresmikan" id="diresmikan" class="" required>
            </div>
            <div class="d-flex flex-column color-skyblue gap-1 mb-4">
                <label for="pulau" id="label-pulau">pulau</label>
                <input type="text" name="pulau" id="pulau" class="bg-white" required>
            </div>
            <div class="d-flex flex-column color-skyblue gap-1 mb-4">
                <label for="photo" id="label-photo">photo</label>
                <input type="file" name="photo" id="photo" class="bg-white" required>
            </div>
            <div class="d-flex flex-column align-items-end gap-1 mb-4">
                <button type="submit" class="btn btn-outline-primary w-25">add</button>
            </div>
        </form>
    </div>
</div>

<%- contentFor("js")  %>